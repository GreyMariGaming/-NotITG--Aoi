	<Layer File="@ThemeFile('ScreenSelectMusic difficulty frame p1.gif')" OnCommand="x,SCREEN_CENTER_X+(140-320);y,SCREEN_CENTER_Y+(347-240)" />
	<Layer File="@ThemeFile('ScreenSelectMusic difficulty frame p1.gif')" OnCommand="x,SCREEN_CENTER_X+(140-320);y,SCREEN_CENTER_Y+(408-240)" />
